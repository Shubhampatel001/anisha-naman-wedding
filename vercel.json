{
  "routes": [
    { "src": "^/assets/(.*)", "dest": "/assets/$1" },
    { "src": "^/intro-poster.webp$", "dest": "/intro-poster.webp" },
    { "src": "^/favicon(.*)", "dest": "/favicon$1" },

    { "src": "^/naman-anisha/?$", "dest": "/naman-anisha.html" },
    { "src": "^/anisha-naman/?$", "dest": "/anisha-naman.html" },

    { "src": "/(.*)", "dest": "/index.html" }
  ]
}